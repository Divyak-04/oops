
class Animal{
    void sound(){
        System.out.print("animals make sound");
    }
    
}
    class Dog extends Animal{
        void bark(){
            System.out.print("dog will bark");
        }
    }


public class Main{
    public static void main(String[] args){
        Dog obj=new Dog();
        
        obj.sound();
        obj.bark();
    }
}
